{
  "name": "@context-engine/cli",
  "version": "2.0.0",
  "description": "Context Engine CLI - Development companion for AI-assisted coding",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ce": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "cli": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "context",
    "ai",
    "development",
    "cli",
    "semantic-anchors",
    "tree-sitter"
  ],
  "author": "Context Engine Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greenmuse123/contextengine.git"
  },
  "bugs": {
    "url": "https://github.com/Greenmuse123/context-engine/issues"
  },
  "homepage": "https://github.com/Greenmuse123/context-engine#readme",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "docs/SETUP.md",
    "docs/COMMANDS.md",
    "docs/PRIVACY.md",
    "docs/ARCHITECTURE.md"
  ],
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.3.7",
    "js-tiktoken": "^1.0.14",
    "ora": "^8.1.0",
    "simple-git": "^3.27.0",
    "web-tree-sitter": "^0.22.6",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
